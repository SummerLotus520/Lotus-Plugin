# 游戏版本周期配置
#
# defaultCycleDays: 普通版本周期的天数（例如：42天）
# specialCycles: 特殊版本周期的设置。这是一个列表，没有特殊情况时写成 []。
#   - version: "x.x.x"  # 对应版本号
#     days: xx          # 该版本的持续天数

ys:
  defaultCycleDays: 42
  specialCycles: [] 

sr:
  defaultCycleDays: 42
  specialCycles: [] 

zzz:
  defaultCycleDays: 42
  specialCycles: 
    - version: "2.1.0"
      days: 50

bh3:
  defaultCycleDays: 42
  specialCycles: []